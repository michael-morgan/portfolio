    "cookie-parser": "~1.4.3",
    "core-js": "^2.4.1",
    "debug": "~2.2.0",
    "express": "~4.14.0",
    "mongodb": "^2.0.43",
    "mongoose": "^4.6.7",
    "morgan": "~1.7.0",
